# Folders
export v_config="$HOME/.config"
export v_desktop="$HOME/Desktop"
export v_downloads="$HOME/Downloads"
export v_documents="$HOME/Documents"
export v_zshrc="$v_config/myconf"
export v_zshrc_base="~/.zshrc"
export v_lunarvim="~/.local/share/lunarvim/lvim"
export v_lunarconfig="$v_config/lvim"
export v_projects="~/projects"
export v_ssd="/mnt/ssd"
export v_hdd="/mnt/hdd"
export v_backup="/mnt/backup"
export v_wprofile="$v_ssd/Users/guilh"
export v_wdocuments="$v_wprofile/Documents"
export v_wdownloads="$v_wprofile/Downloads"
export v_wprojects="$v_hdd/projects"
export v_discordbot="$v_backup/discordbot"
export v_anime="$v_hdd/Animes"
export v_movies="$v_hdd/Movies"
export v_hyde="$v_config/hyde"
export v_scripts="$v_zshrc/scripts"
export v_fstab="/etc/fstab"
